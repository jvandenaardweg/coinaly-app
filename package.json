{
  "name": "coinalyapp",
  "version": "1.0.0",
  "description": "Cryptocurrency trading made simple",
  "author": "Jordy van den Aardweg <jordyvandenaardweg@gmail.com>",
  "private": true,
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0",
    "yarn": "1.5.1"
  },
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "yarn run test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "test:unit": "vue-cli-service test:unit --coverage",
    "test:e2e": "testcafe chrome tests/e2e/*.test.js -S -s screenshots",
    "test:e2e:login": "testcafe chrome tests/e2e/login.test.js -S -s screenshots",
    "test:e2e:signup": "testcafe chrome tests/e2e/signup.test.js -S -s screenshots",
    "test:e2e:forgotpassword": "testcafe chrome tests/e2e/forgot-password.test.js -S -s screenshots",
    "lint:fix": "eslint --fix src/** --ignore-pattern '*.test.js'",
    "heroku-postbuild": "[ $NODE_ENV != test ] && yarn build || exit 0",
    "update-icons": "yarn icons:update && yarn icons:copy",
    "icons:update": "yarn add cjdowner/cryptocurrency-icons#master",
    "icons:copy": "cpx \"./node_modules/cryptocurrency-icons/svg/**\" ./public/static/icons/cryptocurrencies/svg"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "chart.js": "^2.7.2",
    "cryptocurrency-icons": "cjdowner/cryptocurrency-icons#master",
    "date-fns": "^1.29.0",
    "jwt-decode": "^2.2.0",
    "numeral": "^2.0.6",
    "promise.prototype.finally": "^3.1.0",
    "raven-js": "^3.25.2",
    "register-service-worker": "^1.0.0",
    "socketcluster-client": "^13.0.0",
    "vee-validate": "^2.0.9",
    "vue": "^2.5.16",
    "vue-chartjs": "^3.3.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4",
    "vuex-router-sync": "^5.0.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-e2e-nightwatch": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.15",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/eslint-config-standard": "^3.0.0-beta.15",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "cpx": "^1.5.0",
    "dotenv": "^5.0.1",
    "flush-promises": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "nock": "^9.2.6",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "socket.io-client": "^2.1.1",
    "testcafe": "^0.20.1",
    "vue-cli-plugin-critical": "^1.1.0",
    "vue-template-compiler": "^2.5.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
