{
  "root": "dist/",
  "https_only": true,
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/service-worker.js": {
      "Cache-Control": "no-store, no-cache"
    },
    "/index.html": {
      "Cache-Control": "no-store, no-cache"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "coinaly.io"
    },
    "/js/**": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "coinaly.io"
    },
    "/css/**": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "coinaly.io"
    }
  }
}
